{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#1793D1",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#1793D1>\ue0b2</>",
          "properties": {
            "fedora": " Ôåä ",
            "arch": "ÔåÉ Arch",
            "ubuntu": "\uf31b Ubuntu",
            "macos": "\uf179 macOS",
            "windows": "\ue62a Windows"
          },
          "style": "diamond",
          "template": "{{ .Icon }}",
          "type": "os"
        },
        {
          "background": "#FEF5ED",
          "foreground": "#011627",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\uf489 {{ .Name }}",
          "trailing_diamond": "<transparent,#FEF5ED>\ue0b2</>",
          "type": "shell"
        },
        {
          "background": "#1793D1",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "\ue606 {{ .Venv }} ",
          "trailing_diamond": "<transparent,#1793D1>\ue0b2</>",
          "type": "python"
        },
        {
          "background": "#516BEB",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "{{ if .PhysicalTotalMemory }}\ue266 MEM: {{ .PhysicalTotalMemory }}{{ end }}",
          "trailing_diamond": "<transparent,#516BEB>\ue0b2</>",
          "type": "sysinfo"
        },
        {
          "background": "#575656",
          "foreground": "#d6deeb",
          "leading_diamond": "\ue0b2",
          "properties": {
            "display_plugged": true,
            "threshold": 20
          },
          "style": "diamond",
          "template": "{{ if .Percentage }}\uf240 {{ .Percentage }}% {{ end }}",
          "trailing_diamond": "<transparent,#575656>\ue0b2</>",
          "type": "battery"
        },
        {
          "background": "#3A3A3A",
          "foreground": "#d6deeb",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "{{ if .FormattedMs }}‚è± {{ .FormattedMs }}{{ end }}",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "‚ï≠‚îÄ‚îÄ",
          "type": "text"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " ‚ô• {{ .CurrentDate | date .Format }} |",
          "type": "time"
        },
        {
          "style": "plain",
          "template": " \uf292 ",
          "type": "root"
        },
        {
          "properties": {
            "folder_icon": "\uf07b ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\ueb06 ",
            "style": "full"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#011627",
          "foreground": "#98C379",
          "leading_diamond": " <transparent,#011627>\ue0b0</> ",
          "properties": {
            "branch_icon": "\ue725 "
          },
          "style": "diamond",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}",
          "trailing_diamond": " <transparent,#011627>\ue0b0</> ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ï∞‚îÄ‚îÄ ",
          "type": "status"
        },
        {
          "style": "plain",
          "template": "ü¶Ñ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Path }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "\ue285 "
  },
  "version": 3
}
